{
    "extends": "airbnb",
    "rules": {
        "no-plusplus": [
            0
        ],
        "no-console": [
            0
        ],
        "no-unused-expressions": [
            "error",
            {
                "allowTernary": true
            }
        ]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "d3": true
    }
}